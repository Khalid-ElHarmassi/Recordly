<p id="notice"><%= notice %></p>



<table>
  <thead>
  <tr>

    <th>Song title</th>
    <th>Album</th>
    <th>Favorite song</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @song_results.each do |song| %>
      <tr>

        <td><%= song.spng_title %></td>
        <td><%= song.album_id %></td>
        <td><%= song.favorite_song %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<br/>